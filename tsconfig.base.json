{
	"compilerOptions": {
		"strict": true,
		"target": "ES2020",
		"module": "Node16",
		"moduleResolution": "Node16",
		"sourceMap": true,
		"experimentalDecorators": true,
		"pretty": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true
	}
}
